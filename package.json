{
  "name": "renepo",
  "version": "0.0.0",
  "description": "Renepo allows you to open a URL in multiple browsers at the same time using the command line. Super helpful for during web development when you need to open a site in multiple browsers for testing.",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "bin": {
    "renepo": "build/index.js"
  },
  "bugs": {
    "url": "https://github.com/msrcodes/renepo/issues"
  },
  "repository": "git@github.com:msrcodes/renepo.git",
  "author": "Mikael Rozee <mikaelsrozee@gmail.com> (https://msr.codes/)",
  "license": "MIT",
  "keywords": [
    "open",
    "window",
    "opener",
    "browser",
    "testing",
    "development",
    "web",
    "debugging"
  ],
  "scripts": {
    "lint": "gts lint",
    "clean": "gts clean",
    "build": "tsc",
    "fix": "gts fix",
    "prepare": "husky install && yarn build",
    "pretest": "yarn build",
    "posttest": "yarn lint"
  },
  "lint-staged": {
    ".{js,jsx,ts,tsx}": "yarn lint"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.0",
    "@commitlint/config-angular": "^17.0.0",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@types/debug": "^4.1.7",
    "@types/node": "^14.11.2",
    "gts": "^3.1.0",
    "husky": "^8.0.1",
    "lint-staged": "^12.4.1",
    "semantic-release": "^19.0.2",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "chalk": "^5.0.1",
    "commander": "^9.2.0",
    "debug": "^4.3.4",
    "open": "^8.4.0"
  }
}
